{
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "python.analysis.extraPaths": [
        "./src"
    ],
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.direnv/*/**": true,
        "**/wandb/*/**": true,
        "**/results/*/**": true
    },
    "python.formatting.provider": "none"
}
